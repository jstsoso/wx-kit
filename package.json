{
  "name": "wx-kit",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "yarn && cross-env NODE_ENV=development gulp dev",
    "staging": "yarn && cross-env NODE_ENV=staging gulp build",
    "build": "yarn && cross-env NODE_ENV=production gulp build",
    "lint": "yarn && stylelint --max-warnings 0 'src/**/*.less' && eslint --max-warnings 0 src/",
    "deploy:staging": "yarn staging && cross-env NODE_ENV=staging node script/miniprogram-ci.js",
    "deploy:production": "yarn build && cross-env NODE_ENV=production node script/miniprogram-ci.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "commit-msg": "echo $HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "*.less": "stylelint",
    "*.js": "eslint"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.22.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "miniprogram-ci": "^1.0.43",
    "node-notifier": "^7.0.1",
    "postcss-px2units": "^0.2.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "through2": "^4.0.2",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.4",
    "big.js": "^5.2.2",
    "dayjs": "^1.8.28",
    "lodash": "^4.17.15",
    "mobx": "^4.15.4"
  }
}
