{
  "pages": [
    "page/main/home/home",
    "page/main/mine/mine",
    "page/main/webview/webview"
  ],
  "subpackages": [
    {
      "root": "page/order",
      "pages": [
        "list/list"
      ]
    }
  ],
  "preloadRule": {
    "page/main/mine/mine": { "network": "all", "packages": ["page/order"] }
  },
  "window": {
    "navigationBarTitleText": "wx-kit",
    "navigationBarTextStyle": "white",
    "navigationBarBackgroundColor": "#000",
    "navigationStyle": "default",

    "backgroundColor": "#f4f4f4",
    "backgroundColorTop": "#f4f4f4",
    "backgroundColorBottom": "#f4f4f4",
    "backgroundTextStyle": "dark",

    "enablePullDownRefresh": false,
    "onReachBottomDistance": 200,
    "pageOrientation": "portrait"
  },
  "tabBar": {
    "color": "#999",
    "selectedColor": "#fff",
    "backgroundColor": "#000",
    "borderStyle": "black",
    "position": "bottom",
    "custom": false,
    "list": [
      {
        "pagePath": "page/main/home/home",
        "text": "首页"
      },{
        "pagePath": "page/main/mine/mine",
        "text": "我的"
      }
    ]
  },
  "style": "v2",
  "lazyCodeLoading": "requiredComponents",
  "usingComponents": {
    "mask": "/component/mask/index",
    "stepper": "/component/stepper/index",
    "error-page": "/component/error-page/index",
    "custom-item": "/component/custom-item/index",
    "card-item": "/component/card-item/index",
    "img-icon": "/component/img-icon/index",
    "popup": "/component/popup/index",
    "popup-share": "/component/popup-share/index",
    "loading-more": "/component/loading-more/index",
    "fixed-bottom": "/component/fixed-bottom/index"
  }
}
